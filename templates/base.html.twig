<!DOCTYPE html>
<html lang="en-us">

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Kitty Quotes!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600%7CRaleway:300,400,500,600,700%7CLato:300,400,400italic,600,700"
          rel="stylesheet" type="text/css"/>
</head>

<body class="smoothscroll enable-animation">

<div id="wrapper">

    <div id="header" class="navbar-toggleable-md sticky clearfix">

        <!-- TOP NAV -->
        <header id="topNav">
            <div class="container">

                <!-- Logo -->
                <a class="logo float-left" href="/">
                    <h1 class="font-lato fs-30 mt-30">KittyQuotes</h1>
                </a>

                <div class="navbar-collapse collapse float-right nav-main-collapse">
                    <nav class="nav-main">

                        <ul id="topMain" class="nav nav-pills nav-main has-topBar">
                            <li><!-- HOME -->
                                <a class="" href="/">
                                    HOME
                                </a>
                            </li>
                            <li>
                                <a class="" href="{{ path('easyadmin') }}">
                                    ADMIN
                                </a>
                            </li>
                        </ul>

                    </nav>
                </div>

            </div>
        </header>
        <!-- /Top Nav -->

    </div>

    {% block body %}{% endblock %}

</div>

<!-- FOOTER -->
<footer id="footer">

</footer>
<!-- /FOOTER -->

{% block javascripts %}{{ encore_entry_script_tags('app') }}{% endblock %}

</body>
</html>